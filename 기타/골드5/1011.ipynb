   "source": [
    "import sys\n",
    "\n",
    "T = int(input())\n",
    "test_list = []\n",
    "\n",
    "for _ in range(T):\n",
    "    x, y = map(int, input().split())\n",
    "    dis = y - x\n",
    "    n = int((dis)**0.5)\n",
    "    if dis > n * (n+1):\n",
    "        rep = 2 * n + 1\n",
    "    else:\n",
    "        rep = 2 * n\n",
    "    print(rep)"
   ]
